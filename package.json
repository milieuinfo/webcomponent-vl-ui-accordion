{
  "name": "vl-ui-accordion",
  "version": "1.0.13",
  "description": "Gebruik de accordion om informatie te tonen of te verbergen aan de hand van een toggle.",
  "main": "vl-accordion.js",
  "author": "DIDM",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-accordion.git"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-accordion#readme",
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  },
  "scripts": {
    "prepare": "npm update --dev vl-ui-util && npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run util",
    "test": "wct -l chrome,firefox --npm",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:imports && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "uglifyjs --compress --mangle --output vl-accordion.js -- *.src.js",
    "release:prepare:imports": "npm run release:prepare:imports:core && npm run release:prepare:imports:dist",
    "release:prepare:imports:core": "replace '/node_modules/vl-ui-core/vl-core.src.js' '/node_modules/vl-ui-core/vl-core.js' vl-accordion.js",
    "release:prepare:imports:dist": "replace '../dist/accordion.js' '/node_modules/vl-ui-accordion/dist/accordion.js' vl-accordion.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-accordion/style.css' vl-accordion.js",
    "release:prepare:commit": "git add -f vl-accordion.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "dependencies": {
    "@govflanders/vl-ui-accordion": "3.9.1",
    "vl-ui-button": "^1.3.0",
    "vl-ui-core": "^2.1.0",
    "vl-ui-icon": "^2.1.0"
  },
  "peerDependencies": {
    "vl-ui-core": "^2.0.0",
    "vl-ui-button": "^1.1.12",
    "vl-ui-icon": "^2.0.3"
  },
  "devDependencies": {
    "@govflanders/vl-ui-core": "3.9.1",
    "@govflanders/vl-ui-link": "3.9.1",
    "@govflanders/vl-ui-toggle": "3.9.1",
    "@govflanders/vl-ui-util": "3.9.1",
    "copyfiles": "2.1.1",
    "http-server": "0.11.1",
    "np": "3.1.0",
    "replace": "1.1.1",
    "uglify-es": "github:mishoo/UglifyJS2#harmony",
    "vl-ui-util": "^2.0.13",
    "wct-browser-legacy": "1.0.2",
    "web-component-tester": "6.9.2"
  }
}
